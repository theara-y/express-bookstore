{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "amazon_url",
        "author",
        "language",
        "pages",
        "publisher",
        "title",
        "year"
    ],
    "properties": {
        "amazon_url": {
            "type": "string",
            "title": "The amazon_url Schema",
            "examples": [
                "https://www.jsonschema.net/app/schemas/0"
            ]
        },
        "author": {
            "type": "string",
            "title": "The author Schema",
            "examples": [
                "Test Author"
            ]
        },
        "language": {
            "type": "string",
            "title": "The language Schema",
            "examples": [
                "en"
            ]
        },
        "pages": {
            "type": "integer",
            "title": "The pages Schema",
            "examples": [
                100
            ]
        },
        "publisher": {
            "type": "string",
            "title": "The publisher Schema",
            "examples": [
                "Books Published"
            ]
        },
        "title": {
            "type": "string",
            "title": "The title Schema",
            "examples": [
                "My First Book"
            ]
        },
        "year": {
            "type": "integer",
            "title": "The year Schema",
            "examples": [
                2023
            ]
        }
    },
    "examples": [{
        "amazon_url": "https://www.jsonschema.net/app/schemas/0",
        "author": "Test Author",
        "language": "en",
        "pages": 100,
        "publisher": "Books Published",
        "title": "My First Book",
        "year": 2023
    }]
}